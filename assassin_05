#include <stdio.h>

int main() {
    int i;

    // Print numbers from 0 to 10
    i = 0;
    printf("Numbers from 0 to 10:\n");
    while (i <= 10) {
        printf("%d ", i);
        i++;
    }
    printf("\n");

    // Print numbers from 10 to 0
    i = 10;
    printf("Numbers from 10 to 0:\n");
    while (i >= 0) {
        printf("%d ", i);
        i--;
    }
    printf("\n");

    return 0;
}
