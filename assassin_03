#include <stdio.h>

int main() {
    int arr[100];   // Array to store up to 100 elements
    int n, i;

    // Input number of elements
    printf("Enter number of elements: ");
    scanf("%d", &n);

    // Input elements into array
    printf("Enter %d elements:\n", n);
    for (i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }

    // Print negative elements
    printf("Negative elements in the array are:\n");
    for (i = 0; i < n; i++) {
        if (arr[i] < 0) {
            printf("%d ", arr[i]);
        }
    }

    printf("\n"); // Newline after printing

    return 0;
}
