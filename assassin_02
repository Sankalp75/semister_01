#include <stdio.h>

// User-defined function with integer return type and no arguments
int sub() {
    int a, b, result;

    // Input two numbers
    printf("Enter first number: ");
    scanf("%d", &a);

    printf("Enter second number: ");
    scanf("%d", &b);

    // Perform subtraction
    result = a - b;

    // Return result
    return result;
}

int main() {
    int difference;

    // Call the function
    difference = sub();

    // Display result
    printf("Subtraction result = %d\n", difference);

    return 0;
}
